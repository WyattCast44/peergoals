{
    "name": "wyattcast44/peergoals",
    "type": "project",
    "description": "A full stack web application built with the TALL stack.",
    "keywords": ["TALL", "tailwindcss", "alpinejs", "laravel", "livewire"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "blade-ui-kit/blade-icons": "^1.1",
        "blade-ui-kit/blade-ui-kit": "^0.3.1",
        "danharrin/livewire-rate-limiting": "^0.3.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/fortify": "^1.8",
        "laravel/framework": "^8.54",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "league/commonmark": "^1.4",
        "livewire/livewire": "^2.6",
        "spatie/laravel-queueable-action": "^2.12",
        "vinkla/hashids": "^9.1",
        "watson/active": "^6.0",
        "wyattcast44/laravel-safe-username": "^3.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.3",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "laravel/telescope": "^4.6",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
